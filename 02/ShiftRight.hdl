	Mux(a=in[6], b=false, sel=false, out=out[5]);
	Mux(a=in[7], b=false, sel=false, out=out[6]);
	Mux(a=in[8], b=false, sel=false, out=out[7]);
	Mux(a=in[9], b=false, sel=false, out=out[8]);
	Mux(a=in[10], b=false, sel=false, out=out[9]);
	Mux(a=in[11], b=false, sel=false, out=out[10]);
	Mux(a=in[12], b=false, sel=false, out=out[11]);
	Mux(a=in[13], b=false, sel=false, out=out[12]);
